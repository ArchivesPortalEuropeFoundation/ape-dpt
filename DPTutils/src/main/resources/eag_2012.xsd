    <xs:element name="relations">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="resourceRelation" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="eagRelation" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="resourceRelation">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="relationEntry"/>
                <xs:element ref="descriptiveNote" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="resourceRelationType" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="creatorOf"/>
                        <xs:enumeration value="subjectOf"/>
                        <xs:enumeration value="other"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attributeGroup ref="href"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="relationEntry">
        <xs:complexType mixed="true" />
    </xs:element>

    <xs:element name="eagRelation">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="relationEntry" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="descriptiveNote" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="eagRelationType" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="hierarchical-child"/>
                        <xs:enumeration value="hierarchical-parent"/>
                        <xs:enumeration value="temporal-earlier"/>
                        <xs:enumeration value="temporal-later"/>
                        <xs:enumeration value="associative"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attributeGroup ref="href"/>
        </xs:complexType>
    </xs:element>
