<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="1.0+">
    <information>
        <title>WP4 Standalone mapping tool</title>
        <vendor>APEnet project</vendor>
        <homepage href="http://www.apenet.eu/"/>
        <description kind="short">WP4 Standalone mapping tool</description>
    </information>
    <security>
        <all-permissions/>
    </security>
    <resources>
        <j2se version="1.5+" initial-heap-size="256m" max-heap-size="512m"/>
        <property name="jnlp.versionEnabled" value="false"/>
        $dependencies
    </resources>
    <application-desc main-class="$mainClass"/>
</jnlp>
